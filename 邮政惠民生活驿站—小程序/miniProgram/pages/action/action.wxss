.object-box{
  width:100%;
  height:auto;
  position: relative;
  margin-bottom:90rpx;

}

.objectimg{
  display: block;
  width:100%;
  height:auto;
}

.object{
  width:100%;
  height:90rpx;
  background: #66b111;
  color: #fff;
  font-size: 30rpx;
  font-weight: bold;
  text-align: center;
  line-height:90rpx;
  position: fixed;
  bottom:0;
  left:0;

}