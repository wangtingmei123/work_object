/* pages/cart/cart.wxss */
page {
  background: #f8f8f8;
}
button::after {
  border: none;
  border-radius: 0;
}
button {
  border-radius: 0;
}
.container {
  padding: 0;
  margin-top: 20rpx;
  width: 100%;
  padding-bottom: 160rpx;
}
/* 购物车商品 start */
.cart {
  width: 100%;
  height: 160rpx;
  background: #fff;
  border-bottom: 1rpx solid #f8f8f8;
  padding: 20rpx;
  display: flex;
  flex-direction: row;
  position: relative;
}
.cart_left {
  width: 240rpx;
  height: 160rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.ico {
  margin:0 20rpx;
}
.cart_left_img {
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  border-radius: 8rpx;
}
.cart_right {
  position: relative;
  width: 450rpx;
  height: 160rpx;
  padding: 0 20rpx 0 30rpx;

}
.cart_right_title {
  font-size: 28rpx;
  color: #000;
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
margin-top:10rpx;
font-weight: bold;
}
.cart_right_ctn {
  font-size: 26rpx;
  color: #ababab;
  margin-top: 5rpx;
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
}

.price {
  font-size: 30rpx;
  margin-top: 34rpx;
  color:#df382c;
  position: absolute;
  bottom:0rpx;
}

.act_price{
  color: #ff5705;
}

.sale_price{
  font-size: 24rpx;
  color: #ababab;
  text-decoration: line-through;
  margin-left:16rpx;
}



.addReduce {
  position: absolute;
  bottom: 0;
  right: 20rpx;
  width: 200rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: #f8f8f8;
  display: flex;
  flex-direction: row;
}
.math {
  width: 50rpx;
  height: 50rpx;
  background: #f0f0f0;
  position: relative;
  padding: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.num {
  width: 100rpx;
  text-align: center;
  font-size: 24rpx;
}
.reduce_img {
  width: 26rpx;
  height: 4rpx;
}
.add_img {
  width: 26rpx;
  height: 26rpx;
}
/* 购物车商品 end */


.touch-item {
 font-size: 14px;
 display: flex;
 justify-content: space-between;

 width: 100%;
 overflow: hidden;
 position: relative;
}
.content {
 width: 100%;

 line-height: 22px;
 margin-right:0;
 -webkit-transition: all 0.4s;
 transition: all 0.4s;
 -webkit-transform: translateX(90px);
 transform: translateX(90px);
 margin-left: -90px;
  height: 160rpx;
  background: #fff;
  border-bottom: 1rpx solid #f8f8f8;
  padding: 20rpx;
  display: flex;
  flex-direction: row;


}
.del {
 background-color: orangered;
 width: 90px;
 display: flex;
 flex-direction: column;
 align-items: center;
 justify-content: center;
 color: #fff;
 -webkit-transform: translateX(90px);
 transform: translateX(90px);
 -webkit-transition: all 0.4s;
 transition: all 0.4s;
 z-index: 999999;
}
.touch-move-active .content,
.touch-move-active .del {
 -webkit-transform: translateX(0);
 transform: translateX(0);
}






