/* pages/group/group.wxss */

.banner-box{
  width:100%;
  height:250rpx;
  padding: 20rpx;
  padding-right:20rpx;
  box-sizing: border-box;
  border-radius: 6rpx;
}

navigator {
  display: inline;
}

.bannerm{
  display: block;
  width:100%;
  height:100%;
    border-radius: 6rpx;
}
.line {
  width: 100%;
  height: 20rpx;
  background: #f8f8f8;
}
.container {
  width: 100%;
  padding: 0;
  margin: 0;
}
image {
  width: 100%;
  height: 100%;
}
.top {
  width: 100%;
  display: flex;
  justify-content: space-around;
  border-top: 1rpx solid #f0f0f0;
  border-bottom: 1rpx solid #f0f0f0;
}
.top view {
  height: 88rpx;
  width: 50%;
  text-align: center;
  line-height: 88rpx;
  font-size: 30rpx;
}
.active {
  text-shadow: 1rpx 1rpx 5rpx green;
  background:url(data:image/png;base64,) no-repeat;
  background-size: 30% 30%;
  background-size:25% 93%;
  background-position-x:50%;
}
/* 列表区域 start  */
.list_top {
  width: 100%;
}
.list {
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.list_item {
  position: relative;
  display: flex;
  flex-direction: row;
  width: 100%;
  border-bottom: 1rpx solid #f0f0f0;
  padding:15rpx 20rpx;
  background: #fff;
}
.list_item_img_big {
  position: relative;
  width: 180rpx;
  height: 180rpx;
  border-radius: 14rpx;
}
.list_item_img {
  width: 100%;
  height: 100%;
}
.list_item_right {
  margin-left: 30rpx;
  width: 400rpx;
  height: 170rpx;
  padding-bottom: 10rpx;
  position: relative;
}
.list_item_right_title {
  white-space:nowrap; 
  overflow:hidden;   
  text-overflow:ellipsis;
  font-size: 30rpx;
  color: #000;
  margin-top:10rpx;
  font-weight: bold;
}
.list_item_right_ctn {
  white-space:nowrap; 
  overflow:hidden;   
  text-overflow:ellipsis;
margin-top:5rpx;
  font-size: 26rpx;
  color: #ababab;
}
.list_item_right_hb_big {
  width: 100%;
}
.list_item_right_hb {
  font-size: 24rpx;
  color: #ababab;
  border: 1rpx solid #ababab;
  border-radius: 6rpx;
  padding: 2rpx;
  margin-top: 50rpx;
  float: left;
}
.list_item_right_price {
  position: absolute;
  
 bottom:10rpx;
}
.list_item_right_newPrice {
  font-size: 30rpx;
  color: #ff5705;
  margin-right: 10rpx;
  margin-left: -12rpx;
}
.list_item_right_oldPrice {
  font-size: 24rpx;
  color: #ababab;
  text-decoration: line-through;
}
.cart {
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  width: 140rpx;
  height: 60rpx;
  border-radius: 50%;
}
.cart_but {
  height: 60rpx;
  width: auto;
  border-radius: 30rpx;
  background: repeating-radial-gradient(#6ab614,#9cc912);
  font-size: 26rpx;
  color:#fff;
  line-height: 60rpx;
  text-align: center;
  padding-left:20rpx;
  padding-right:20rpx;
}
.cart_img {
  width: 100%;
  height: 100%;
}
.promotion_big {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  left: -8rpx;
  top: 12rpx;
  width: 61rpx;
  height: 57rpx;
}
.promotion_bg {
  width: 100%;
  height: 100%;
}
.promotion_ctn {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
}
/* 列表区域 end  */
/* 今日团购 start */
.list_item2 {
  width: 100%;
}
.list_item2_big {
  border-bottom: 1rpx solid #f8f8f8;
  padding: 20rpx 20rpx 20rpx;
}
.list_item2_img_big {
  position: relative;
  width: 100%;
  display: flex;
  justify-content: center;
}
.list_item2_img {
  width: 310rpx;
  height: 310rpx;
}
.list_item2_ctn {
  position: relative;
  width: 100%;
  padding: 20rpx 0 0rpx;
  display: block;
}
.list_item2_title {
  font-size: 30rpx;
  color: #000;
}
.list_item2_content {
  font-size: 26rpx;
  color: #ababab;
  margin-top: 10rpx;
}
.list_item2_price {
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  margin-top: 20rpx;
  color: #feb839;
}
.list_item2_newprice {
  line-height: 1;
  margin-left: 10rpx;
}
.list_item2_oldprice {
  margin-left: 10rpx;
  font-size: 24rpx;
  color: #ababab;
  text-decoration: line-through;
}
