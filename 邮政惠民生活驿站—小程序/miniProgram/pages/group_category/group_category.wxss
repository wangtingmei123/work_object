/* pages/group_category/group_category.wxss */
page{
    width: 100%;
    height: 100%;
    background: #f8f8f8;
}
image {
  width: 100%;
  height: 100%;
}
.clearfix::after {
  display: block;
  clear: both;
  content: "";
}
.container{
    width: 100%;
    height: 100%;
    padding: 0;
}
/* 导航 start */
.nav {
    height: 80rpx;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 80rpx;
    background: #fff;
    font-size: 16px;
    white-space: nowrap;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99999;
    border-top: 1rpx solid #f8f8f8;
    /* text-align: center; */
}
.nav-item {
    width: auto;
    display: inline-block;
    text-align: center;
    box-sizing: border-box;
    padding-left:30rpx;
    padding-right:30rpx;
    
}

.nav-item.active{
    color: #61b10d;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 44% 10rpx;
    background-position: 50% 90%;
}
/* 导航 end */
/* 轮播图 start  */
.swip {
  margin-top: 100rpx;
  height: 180rpx;
  width: 100%;
}
.swip image {
  width: 100%;
  height: 180rpx;
  border-radius: 14rpx;
}
/* 轮播图 end  */
/* 列表 start */
.item_big {
  width:100%;
  height: 100%;
}
.list_item {
  position: relative;
  display: flex;
  flex-direction: row;
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f0f0f0;
  padding:15rpx 20rpx;
  background: #fff;
}
.list_item_img_big {
  position: relative;
  width: 180rpx;
  height: 180rpx;
  border-radius: 14rpx;
  background: #ace;
}
.list_item_img {
  width: 100%;
  height: 100%;
}
.list_item_right {
  margin-left: 30rpx;
  width: 400rpx;
  height: 170rpx;
  padding-bottom: 10rpx;
  position: relative;
}
.list_item_right_title {
  white-space:nowrap; 
  overflow:hidden;   
  text-overflow:ellipsis;
  font-size: 30rpx;
  color: #000;
font-weight: bold;
}
.list_item_right_ctn {
  white-space:nowrap; 
  overflow:hidden;   
  text-overflow:ellipsis;
  margin-top:5rpx;
  font-size: 26rpx;
  color: #ababab;
}
.list_item_right_hb_big {
  width: auto;
  position: absolute;
  bottom:50rpx;
  left:0;
}
.list_item_right_hb {
  font-size: 22rpx;
  color: #66b111;
  border: 1rpx solid #66b111;
  border-radius: 6rpx;
  padding: 2rpx;
}


.list_item_right_Price{
  width:auto;
  height:30rpx;
    position: absolute;
  bottom:20rpx;

}


.list_item_right_newPrice {
  font-size: 30rpx;
  color: #ff5705;

}
.list_item_right_oldPrice {
  font-size: 24rpx;
  color: #ababab;
  text-decoration: line-through;
  margin-left:20rpx;
}
.cart {
  position: absolute;
  right: 20rpx;
  bottom: 15rpx;
  width: 78rpx;
  height: 78rpx;
  border-radius: 50%;
  overflow: hidden;
}
.cart_but {
  height: 100%;
  width: 100%;
}
.cart_img {
  width: 100%;
  height: 100%;
}
.promotion_big {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  left: -8rpx;
  top: 12rpx;
  width: 61rpx;
  height: 57rpx;
}
.promotion_bg {
  width: 100%;
  height: 100%;
}
.promotion_ctn {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
}
/* 列表 end */
.cart_fixed {
  position: fixed;
  bottom: 70rpx;
  left: 30rpx;
  width: 100rpx;
  height: 100rpx;
  box-shadow: 0 0 10px #999;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
}
.cart_absolute {
  position: absolute;
  right: 6rpx;
  top: 6rpx;
  border-radius: 15rpx;
  width: 30rpx;
  height: 30rpx;
  background: #f80015;
  color: #fff;
  font-size: 20rpx;
  line-height: 30rpx;
  text-align: center;
  white-space:nowrap;
  overflow:hidden;   
  text-overflow:ellipsis;
}
.cart_fixed image {
  width: 50%;
  height: 50%;
}




