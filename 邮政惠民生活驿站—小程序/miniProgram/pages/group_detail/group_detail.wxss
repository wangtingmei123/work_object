/* pages/group_detail/group_detail.wxss */
page {
  background: #f8f8f8;
  height: 100%;
}
.container {
  width: 100%;
  padding: 0;
  background: #f8f8f8;
}
/* 顶部轮播图 start */
.top_big {
  position: relative;
  width: 100%;
  border-bottom: 1rpx solid #f0f0f0;
  padding: 20rpx 0;
  background: #fff;
}
.top_icon_big {
  position:absolute;
  left:56rpx;
  top:20rpx;
  width:100rpx;
  height:100rpx;
}
.top_icon {
  width: 100%;
  height: 100%;
}
.top_icon_ctn {
  position:absolute;
  top:32rpx;
  left:17rpx;
  width:80rpx;
  height:50rpx;
  font-size:32rpx;
  color:#fff;
  line-height:1;
  text-shadow:1rpx 1rpx 5rpx orange;
}

.backTime {
  position: absolute;
  top:450rpx;
  right:0;
  width: 460rpx;
  height: 58rpx;
  border-top-left-radius: 30rpx;
  border-bottom-left-radius: 30rpx;
  border: 1rpx solid #f9e4d6;
  border-right: 0;
  z-index: 3;
  background: #fff;
  line-height: 58rpx;
  text-align: center;
  font-size: 26rpx;
  color: orange;
}

.backTime text{
  color: orange;
}
.fontColor {
  color:#333;
}
.swip {
  position: relative;
  height: 520rpx;
  width: 100%;
}
.img_big {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.slide-image {
  display: block;
  height: 520rpx;
  width:100%;
  margin:auto;
}
/* 顶部轮播图 end */
/* 中部 start */
.middle_big {
  width: 100%;
  margin-bottom: 20rpx;
  background: #fff;
}
.middle {
  padding: 30rpx 20rpx 40rpx;
  display: flex;
  flex-direction: column;
  align-content: space-between;
  height: auto;
}
.middle_tittle {
  font-size: 36rpx;
}
.middle_ctn {
  font-size: 28rpx;
  color: #ababab;
  margin-top: 10rpx;
}
.price_big {
  padding-top:60rpx;
  display:flex;
  align-items:flex-end;
}
.group {
  font-size: 22rpx;
  display: inline-block;
  padding: 5rpx;
  border: 1rpx solid #fd8585;
  color: #fd8585;
  border-radius: 10rpx;
  line-height: 30rpx;
}
.new_price {
  color: #fadc9f;
  margin-left: 10rpx;
  line-height: 1;
}
.old_price {
  color: #ababab;
  font-size: 24rpx;
  margin-top: 15rpx;
  margin-left: 10rpx;
  line-height:1.1;
  text-decoration:line-through;
}
.detail_big {
  margin-top: 20rpx;
  width: 100%;
  background:linear-gradient(to right,#f8e09f,#f2b883) ;
  border-radius: 20rpx;
}
.detail {
  padding: 20rpx;
  font-size: 26rpx;
  color: #401701;
  display: flex;
  justify-content: space-between;
  align-content: cneter;
}
.detail_right_icon {
  width: 20rpx;
  height: 20rpx;
}
/* 中部 end */
/* 拼团人数 start */
.per_big {
  width: 100%;
  margin-bottom: 20rpx;
  background: #fff;
}
.per_top {
  border-bottom: 1rpx solid #f8f8f8;
  padding: 25rpx 20rpx;
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
}
.per_for {
  width: 100%;
  height: 160rpx;
  box-sizing: border-box;
}
.per {
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.per_left {
  display: flex;
  align-items: center;
}
.per_left_img {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  background: #ace;
}
.per_name {
  margin-left: 20rpx;
  font-size: 28rpx;
}
.per_right {
  height: 90rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.per_right_ctn {
  padding: 10rpx 0;
  line-height: 36rpx;
  font-size: 24rpx;
  color: e43636;
}
.butt {
  width: 130rpx;
  height: 50rpx;
  font-size: 24rpx;
  border-radius: 25rpx;
  background: linear-gradient(to right,#ffb92e,#fc9029) ;
  line-height: 50rpx;
  color: #fff;
  margin-left: 20rpx;
}
.butt:after {
  border: none;
}
/* 拼团人数 end */
/* 商品详情 start */
.goods_ctn {
  width: 100%;
  padding: 30rpx 20rpx;
  background: #fff;
  box-sizing: border-box;
  margin-bottom:100rpx;
}
.goods_ctn view {
  color:#ababab;
  font-size: 28rpx;
  margin-top: 30rpx;
}
.goods_ctn .goods_title {
  color: #000;
  margin-top: 0;
}
.goods_img {
  width: 710rpx;
  height: 710rpx;
  margin-top: 20rpx; 
}
/* 底部固定 start */
.bottom {
  position: fixed;
  display: flex;
  flex-direction: row;
  bottom: 0;
  width: 100%;
  height: 99rpx;
  border-top: 1rpx solid #f0f0f0;
  background: #fff;
}
.bottom_icon {
  width: 99rpx;
  height: 99rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.bottom_icon_img {
  width: 56rpx;
  height: 56rpx;
}
.index {
  font-size: 26rpx;
  margin-top: 5rpx;
}
.bottom_btn {
  width: 614rpx;
  height: 69rpx;
  padding: 15rpx 0rpx 15rpx 15rpx;
  display: flex;
  flex-direction: row;
  font-size: 26rpx;
  color: #fff;
  text-shadow: 1rpx 1rpx 5rpx #e66036;
}
.per_buy {
  flex:1;
  line-height: 68rpx;
  border-radius: 0;
  border-top-left-radius: 34rpx;
  border-bottom-left-radius: 34rpx;
  background: #f39429;
  font-size: 26rpx;
  color: #fff;
}
.per_buy::after {
  border:none;
}
.group_buy {
  flex: 1;
  line-height: 68rpx;
  border-radius: 0;
  border-top-right-radius: 34rpx;
  border-bottom-right-radius: 34rpx;
  background: #feb734;
  font-size: 28rpx;
  color: #fff;
}
.group_buy::after {
  border:none;
}
/* 底部固定 end */
/* 商品详情 end */
/* 遮罩层 start */
.mask_big {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.4);
}
.mask {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 640rpx;
  padding: 30rpx 20rpx;
  background: #fff;
  z-index: 11;
}
.mask_title {
  display: flex;
  justify-content: space-between;
}
.space {
  width: 40rpx;
  height: 40rpx;
}
.mask_title_ctn {
  font-size: 28rpx;
  color: #000;
}
.mask_title_icon_big {
  width: 40rpx;
  height: 40rpx;
}
.mask_title_icon_big image {
  width: 100%;
  height: 100%;
}
.per_num {
  width: 100%;
  padding: 30rpx 0 15rpx;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  margin-bottom: 10rpx;
}
.join_per {
  width: 90rpx;
  height: 90rpx;
  margin: 0 30rpx 15rpx;
  border-radius: 50%;
  background: #edd;
  overflow: hidden;
}
.join_per image {
  width: 100%;
  height: 100%;
}
.mask_btn {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: center;
}
.buttt {
  width: 130rpx;
  height: 50rpx;
  font-size: 24rpx;
  border-radius: 25rpx;
  background: linear-gradient(to right,#ffb92e,#fc9029) ;
  color: #fff;
  line-height: 50rpx;
}
.buttt:after {
  border: none;
}

.bottom_wks{
