}
.container {
  padding:0;
  display: flex;
  align-items: flex-start;
}
.img-box {
  width: 100%;
  height: 210rpx;
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 100% 100%;
}
.font {
  margin: 50rpx 0 0 40rpx;
  font-size: 55rpx;
  color: #fff;
}
.req {
  width: 100%;
  box-sizing: border-box;
  padding: 45rpx 40rpx;
  font-size: 24rpx;
  border-bottom: 1rpx solid #f3f3f3;   
}
.req_title {
  font-size: 30rpx;
  font-weight: 500;
  padding-bottom: 30rpx; 
}
.req_content {
  line-height: 1.7;
}
