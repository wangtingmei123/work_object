/* pages/banner_detail/banner_detail.wxss */
.container {
  width: 100%;
  padding: 0;
}
.img {
  padding: 0 20rpx;
  width: 710rpx;
  height: 850rpx;
  background: #ace;
}
/* 列表区域 start  */
.list_top {
  margin-top: 10rpx;
  width: 100%;
}
.list {
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.list_item {
  position: relative;
  display: flex;
  flex-direction: row;
  width: 100%;
  border-bottom: 1rpx solid #f0f0f0;
  padding:15rpx 20rpx;
}
.list_item_img_big {
  position: relative;
  width: 180rpx;
  height: 180rpx;
  border-radius: 14rpx;
  background: #ace;
}
.list_item_img {
  width: 100%;
  height: 100%;
}
.list_item_right {
  margin-left: 30rpx;
  width: 400rpx;
  height: 170rpx;
  padding-bottom: 10rpx;
}
.list_item_right_title {
  white-space:nowrap; 
  overflow:hidden;   
  text-overflow:ellipsis;
  font-size: 30rpx;
  color: #000;
}
.list_item_right_ctn {
  white-space:nowrap; 
  overflow:hidden;   
  text-overflow:ellipsis;
  font-size: 28rpx;
  color: #ababab;
}
.list_item_right_hb_big {
  width: 100%;
}
.list_item_right_hb {
  font-size: 24rpx;
  color: #ababab;
  border: 1rpx solid #ababab;
  border-radius: 6rpx;
  padding: 2rpx;
  margin-top: 22rpx;
  float: left;
}
.list_item_right_price {
  line-height: 1;
}
.list_item_right_newPrice {
  font-size: 30rpx;
  color: #feb839;
  margin-right: 20rpx;
}
.list_item_right_oldPrice {
  font-size: 24rpx;
  color: #ababab;
  text-decoration: line-through;
}
.cart {
  position: absolute;
  right: 15rpx;
  bottom: 15rpx;
  width: 78rpx;
  height: 78rpx;
  border-radius: 50%;
}
.cart_img {
  width: 100%;
  height: 100%;
}
.promotion_big {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  left: -8rpx;
  top: 12rpx;
  width: 61rpx;
  height: 57rpx;
}
.promotion_bg {
  width: 100%;
  height: 100%;
}
.promotion_ctn {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
}
/* 列表区域 end  */


.nonet-box{
  width: 100%;
  height:100vh;
  font-size: 28rpx;
  line-height:300rpx;
  position: fixed;
  top:0;
  bottom:0;
  right:0;
  left:0;
  margin:auto;
  text-align: center;
  background: #fff;
  z-index: 99999;

}

.nonet{
    width: 100%;
  height:600rpx;
  font-size: 28rpx;
  position: absolute;

  right:0;
  left:0;
  margin:auto;
    top:132rpx;

  background: #fff;
  z-index: 99999;
}

.nonet>image{
  display: block;
  width: 300rpx;
  height:305rpx;
  margin:auto;

}

.nontip{
  width:100%;
  height:28rpx;
  margin:auto;
  margin-top:60rpx;
    text-align: center;
      font-size: 28rpx;
      color: #999999;
      line-height: 28rpx;
}

.jiazai{
width: 300rpx;
height:78rpx;
margin:auto;
margin-top:130rpx;
border:1rpx solid #dcdcdc;
text-align: center;
line-height: 78rpx;
  font-size: 28rpx;
}



