/* pages/object/object.wxss */


.object-box{
 width: 100vw;
   height: 100vh;
  position: relative;

}

.object-box>image{
  display: block;
   width: 100vw;
   height: 100vh;

}

.object{
  width:682rpx;
  height:462rpx;
  position: absolute;
  left:0;
  right:0;
  margin:auto;
  bottom:252rpx;

}

.object1{
  width:100%;
  height:202rpx;
  position: relative;
}


.objectimg{
  display: block;
  width:100%;
  height:100%;
}

.objm1{
  width:436rpx;
  position: absolute;
  top:68rpx;
  right:32rpx;
  height:auto;
}

.objm1>.objtop{
  width:auto;
  height:30rpx;
  font-size: 28rpx;
  color: #390606;
  font-weight: bold;

}


.objm1>.objbot{
  width:100%;
  height:46rpx;
  font-size: 28rpx;
  color: #a72116;
    margin-top:25rpx;
  font-weight: bold;
}

.objm1>.objbot>.objbleft{
  width:auto;
  height:100%;
    font-size: 30rpx;
    line-height:46rpx;
    float: left;

}

.objm1>.objbot>.objbleft>text{
  color: #390606;
  margin-left:20rpx;
  text-decoration: line-through;
  font-weight: normal;
  font-size: 22rpx;
}

.objm1>.objbot>.objbright{
  width:125rpx;
  height:100%;
    font-size: 24rpx;
    line-height:46rpx;
    float: right;
    background: #ce341f;
    text-align: center;
    color: #fff;
    font-weight: bold;
    border-radius: 10rpx;
    margin-right:60rpx;

}


