.bottom {
  position: fixed;
  display: flex;
  flex-direction: row;
  bottom: 0;
  width: 100%;
  height: 99rpx;

}
.bottom_icon {
  width: 99rpx;
  height: 99rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.bottom_icon_img {
  width: 56rpx;
  height: 56rpx;
}
.index {
  font-size: 26rpx;
  margin-top: 5rpx;
  position: relative;
}
.bottom_btn {
  width: 614rpx;
  height: 69rpx;
  padding: 15rpx 0rpx 15rpx 15rpx;
  display: flex;
  flex-direction: row;
  font-size: 26rpx;
  color: #fff;
  text-shadow: 1rpx 1rpx 5rpx #357912;
}
.per_buy {
  flex:1;
  line-height: 68rpx;
  border-radius: 0;
  border-top-left-radius: 34rpx;
  border-bottom-left-radius: 34rpx;
  background: #61b012;
  font-size: 26rpx;
  color: #fff;
}
.per_buy::after {
  border:none;
}
.group_buy {
  flex: 1;
  line-height: 68rpx;
  border-radius: 0;
  border-top-right-radius: 34rpx;
  border-bottom-right-radius: 34rpx;
  background: #9cc812;
  font-size: 28rpx;
  color: #fff;
}
.group_buy::after {
  border:none;
}
.num_icon {
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background: #e33636;
  color: #fff;
  font-size: 24rpx;
  border-radius: 50%;
  white-space:nowrap; 
  overflow:hidden;    
  text-overflow:ellipsis;
}
