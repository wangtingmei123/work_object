/**index.wxss**/
view,text,input {
  font-family: PingFangSC-Regular;
}

page{
  background: #f8f8f8;
}
navigator {
  display: inline;
  
}
.clearfix:after {
   content:""; 
   display: block; 
   clear:both; 
}
.container {
  padding: 0;
  font-family: PingFangSC-Regular;
}
.top {
  padding: 2rpx 20rpx 0rpx;
}
image {
  width: 100%;
  height: 100%;
}
/* 顶部搜索start  */
.address_big {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.address_icon {
  width: 22rpx;
  height: 28rpx;
}
.address_left {
  width: 230rpx;
  display: flex;
  align-items: center;
}
.address_ctn {
  display: inline-block;
  width: 162rpx;
  font-size: 28rpx;
  white-space:nowrap; 
  overflow:hidden;   
  text-overflow:ellipsis;
  margin-left: 15rpx;
  color: #000;
}
.address_icon_down {
  width: 14rpx;
  height: 8rpx;
}
.ser_big {
  display: flex;
  width: 450rpx;
  height: 50rpx;
  justify-content: center;
  align-items: center;
}
.input_search {
  width: 450rpx;
}
.ser {
  position: relative;
  width: 390rpx;
  background: #fff;
  border-radius: 25rpx;
  border: 1rpx solid #ababab;
  padding-left:60rpx;
  font-family: PingFangSC-Regular;
}
.placeholder {
  font-size: 24rpx;
  color: #ababab;
}
.fangdajing {
  position: absolute;
  top: 0;
  left: 20rpx;
  width: 26rpx;
  height: 26rpx;
  z-index: 2;
}
.fangdajing_img {
  width: 26rpx;
  height: auto;
}

/* 顶部搜索end  */
/* 轮播图 start  */
.swip {
  height: 180rpx;
  width: 100%;
  margin-top: 23rpx;
}
.swip image {
  width: 100%;
  height: 100%;
  border-radius: 14rpx;
}
/* 轮播图 end  */
/* 分类区域 start  */
.category_big {
  width: 100%;
  margin-top: 30rpx;
  display: flex;
  align-content: space-between;
  flex-wrap: wrap;
}
.category {
  box-sizing: border-box;
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
  white-space:nowrap; 
  overflow:hidden;   
  text-overflow:ellipsis;
}
.category_img {
  width: 120rpx;
  height: 120rpx;
}
.category_ctn {
  margin-top: 10rpx;
  font-size: 28rpx;
  color: #000;
}
/* 分类区域 end  */
/* 今日热卖 start */
.today_hot {
  margin-top: 50rpx auto 0;
  width: 240rpx;
  height: 60rpx;
}
.today_hot_img {
  margin-top: 50rpx;
  width: 100%;
  height: 100%;
}
/* 今日热卖 end */
/* 列表区域 start  */
.list_top {
  margin-top: 28rpx;
  width: 100%;
}
.list {
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.list_item {
  position: relative;
  display: flex;
  flex-direction: row;
  width: 100%;
  border-bottom: 1rpx solid #f0f0f0;
  padding:15rpx 20rpx;
  box-sizing: border-box;
}
.list_item_img_big {
  position: relative;
  width: 180rpx;
  height: 180rpx;
  border-radius: 14rpx;
}
.list_item_img {
  width: 100%;
  height: 100%;
}
.list_item_right {
  margin-left: 30rpx;
  width: 400rpx;
  height: 170rpx;
  padding-bottom: 10rpx;
  position: relative;
}
.list_item_right_title {
  white-space:nowrap; 
  overflow:hidden;   
  text-overflow:ellipsis;
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
}
.list_item_right_ctn {
  white-space:nowrap; 
  overflow:hidden;   
  text-overflow:ellipsis;
  font-size: 26rpx;
  margin-top:5rpx;
  color: #ababab;
}
.list_item_right_hb_big {
  width: auto;
  position: absolute;
  bottom:50rpx;
  left:0;
}
.list_item_right_hb {
  font-size: 22rpx;
  color: #66b111;
  border: 0.2rpx solid #66b111;
  border-radius: 6rpx;
  padding: 2rpx;
  float: left;
}
.list_item_right_price {
  position: absolute;
  bottom:0rpx;
 
}
.list_item_right_newPrice {
  font-size: 30rpx;
  color: #ff5705;
  margin-right: 20rpx;
  margin-top:30rpx;
}
.list_item_right_oldPrice {
  font-size: 24rpx;
  color: #ababab;
  text-decoration: line-through;
}

.xsjiage{
  position: absolute;
  bottom:0rpx;
}


.cart {
  position: absolute;
  right: 15rpx;
  bottom: 15rpx;
  width: 78rpx;
  height: 78rpx;
  border-radius: 50%;
}
.cart_img {
  width: 100%;
  height: 100%;
}
.promotion_big {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  left: -8rpx;
  top: 12rpx;
  width: 61rpx;
  height: 57rpx;
}
.promotion_bg {
  width: 100%;
  height: 100%;
}
.promotion_ctn {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
}
