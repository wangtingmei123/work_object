.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 100rpx;
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
    background: #fff
}
.tabbar_box.iphoneX-height{
    padding-bottom: 66rpx;
}
.middle-wrapper{
  position: absolute;
  right: 310rpx;
  bottom: 0;
  background-color: #fff;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
}
.middle-wrapper.iphoneX-height{
  bottom: 66rpx;
}
.tabbar_nav{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    height: 100%;
    position: relative;
}

.tabbar_nav>text{
  font-size: 20rpx
}



.tabbar_icon{
    width: 56rpx;
    height: 56rpx;
}
.special-wrapper{
  position: absolute;
  top: -66rpx;
  width: 126rpx;
  height: 126rpx;
  border-radius: 50%;
  border-top: 2rpx solid #e9e9e9;
  background-color: #fff;
  text-align: center;
  box-sizing: border-box;
  padding: 6rpx;
  box-shadow: 0 -1px 2px #f3f3f3;

  
}
.special-wrapper .tabbar_icon{
    width: 114rpx;
    height: 114rpx;
}
.special-text-wrapper{
  width: 46rpx;
  height: 46rpx;
}

